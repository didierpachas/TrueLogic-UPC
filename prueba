        int totalVendido = obtenerTotalVentas(ventas);
        System.out.println("Total de Unidades Vendidas: " + totalVendido);

        double porcentaje = obtenerPorcentajeSupero(ventas);
        System.out.println("El porcentaje que super√≥ la cuota fue: " + porcentaje);

        String[] supremos = nombresVendSupCuota(vendedores, ventas);
        System.out.println(Array.toString(supremos));
        System.out.println(listaVendSupCuota(vendedores, ventas));
        relacionVendSupCuota(vendedores, ventas);
    }

